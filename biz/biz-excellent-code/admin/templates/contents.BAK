<!--
{literal}
function delete_conf(id)
{
	if (confirm("削除してもよろしいですか？")) {
{/literal}
		window.location = '?act={$form.name}_delete&id=' + id;
{literal}
	}
}
function do_page(page)
{
{/literal}
	location = '?act={$form.name}_edit&id={$id}&page=' + page;
{literal}
}
function do_hide(items, btn)
{
	var i, item;
	var open = 0;
	ary = items.split(',');
	for (i = 0; i < ary.length; i++) {
		item = document.getElementById("tr_" + ary[i]);
		if (item) {
			if (item.style.display == "none") {
				item.style.display = "";
				open = 1;
			} else {
				item.style.display = "none";
			}
		}
	}
 	item = document.getElementById(btn);
	if (open) {
		item.src = 'open.gif';
	} else {
		item.src = 'close.gif';
	}
}
{/literal}
//-->
